<script type='text/javascript'>
  
$(document).ready(function(){

$(window).scroll(function(){
    if ($(this).scrollTop() > 100) {
        $('.scrollup').fadeIn();
    } else
    {
        $('.scrollup').fadeOut();
    }

});
 
$('.scrollup').click(function(){
    $("html, body").animate({ scrollTop: 0 }, 600);
$("#PlusOne1").effect("highlight", {}, 3000);
    return false;
});

$('.likeus').click(function(){
    $("html, body").animate({ scrollTop: $(document).height() }, 600);
$("#HTML3").effect("highlight", {}, 3000);
    return false;
});

});
</script>
<style>
.scrollup{
    width:46px;
    height:46px;
    position:fixed;
    bottom:50%;
    right:2px;
    display:none;
    text-indent:-9999px;
    background: url('http://2.bp.blogspot.com/-5nHSwSptO7o/VUnwbEG7zvI/AAAAAAAATVw/xWr7Ps1okg8/s1600/scrollTop.png') no-repeat;
    z-index:9999;
    cursor:pointer;
	
	opacity: 0.3;
}

.scrollup:hover{
	opacity: 1;
}

.likeus{
    width:66px;
    height:66px;
    position:fixed;
    bottom:5px;
    left:10px;
    display:none;
    text-indent:-9999px;
	z-index:9999;
	cursor:pointer;
}
</style>
